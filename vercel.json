{
  "version": 2,
  "builds": [
    {
      "src": "elevenlabs-uipath-middleware.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/check-availability",
      "dest": "/elevenlabs-uipath-middleware.js",
